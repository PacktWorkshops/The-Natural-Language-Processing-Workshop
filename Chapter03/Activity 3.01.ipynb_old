    "MAX_FEATURES = 200\n",
    "tfidf_model = TfidfVectorizer(max_features=MAX_FEATURES)\n",
    "tfidf_df = pd.DataFrame(tfidf_model.fit_transform(data['cleaned_headline_text']).todense())\n",
    "tfidf_df.columns = sorted(tfidf_model.vocabulary_)\n",
    "tfidf_df.head()"
   ]
